<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomTextView">

        <attr name="font" format="enum">
            <enum name="san_francisco_display" value="1"/>
            <enum name="sf_ui_display" value="2"/>
            <enum name="sf_ui_text" value="3"/>
        </attr>

        <attr name="textStyle" format="enum">
            <enum name="normal" value="0"/>
            <enum name="bold" value="1"/>
            <enum name="semibold" value="4"/>
        </attr>

    </declare-styleable>
</resources>